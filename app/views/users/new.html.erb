<h1>New user</h1>

<%= form_for @user do |f| %>
  <label>Email</label>
  <%= f.text_field :username %>
  <label>Password</label>
  <%= f.password_field :password %>
  <label>Password Confirmation</label>
  <%= f.password_field :password_confirmation %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back', users_path %>
